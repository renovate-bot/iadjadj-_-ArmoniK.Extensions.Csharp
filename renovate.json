{
  "forkProcessing": "enabled",
  
  "schedule": [ "every weekday" ],
  
  "packageRules": [
    {
      "matchPackagePrefixes": ["ArmoniK"],
      "includeForks": true,
      "groupName": "Group Armonik nuget"
    },
    {
      "groupName": "MyProject Docker Images",
      "includeForks": true,
      "excludePackageNames": ["^baseimage\\.", "^microsoft\\."]
    }
  ]
}
